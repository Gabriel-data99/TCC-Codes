SELECT

 dados.ano as ano,

 dados.id_municipio AS id_municipio,

 diretorio_id_municipio.nome AS id_municipio_nome,

 dados.sexo as sexo,

 dados.grupo_idade as grupo_idade,

 dados.populacao as populacao

FROM `basedosdados.br_ms_populacao.municipio` AS dados

LEFT JOIN (SELECT DISTINCT id_municipio,nome FROM 

`basedosdados.br_bd_diretorios_brasil.municipio`) AS diretorio_id_municipio

 ON dados.id_municipio = diretorio_id_municipio.id_municipio

 WHERE ano = 2021
